        "editor.insertSpaces": false,
        "editor.detectIndentation": false,
        "editor.tabSize": 4
    },
    "emmylua.codeStyle.configPath":{
        "indentation": {
            "useTab": true,
            "indentSize": 4
        },
        "lineLength": 200,
        "callArgs": "chopdown"
    }
}